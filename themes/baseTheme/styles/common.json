    ".bottomleft": {
      "height": null,
      "bottom": 0,
      "left": 0
    },
    ".bottomright": {
      "height": null,
      "bottom": 0,
      "right": 0
    },
    "flexProps": {
      "positionType": "ABSOLUTE"
    }
  },
  ".centerChild": {
    "flexProps": {
      "justifyContent": "CENTER"
    }
  },
  ".bgBlack": {
